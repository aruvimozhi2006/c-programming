#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int size;
    scanf("%d",&size);
    int even = 0, Odd = 0;
    int n;
    for(int i=0;i<size;i++)
    {
        scanf("%d",&n);
        if(n%2==0)
            even=1;
        else
            Odd=1;
    }
    if(even && Odd)
    printf("Mixed\n");
    else if(even)
        printf("Even\n");
    else
        printf("Odd\n");
/* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
